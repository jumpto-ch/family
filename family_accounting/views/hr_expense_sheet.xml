<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="family_accounting_expense_sheet_form" model="ir.ui.view">
        <field name="model">hr.expense.sheet</field>
        <field name="inherit_id" ref="hr_expense.view_hr_expense_sheet_form"/>
        <field name="arch" type="xml">
            <xpath expr="/form/sheet/notebook/page[@name='expenses']/field[@name='expense_line_ids']/tree/field[@name='company_id']"
                   position="after">
                <field name="vendor_id"/>
            </xpath>
        </field>
    </record>
</odoo>